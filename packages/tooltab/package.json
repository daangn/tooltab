{
  "name": "tooltab",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/tooltab.git",
    "directory": "packages/tooltab"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "require": "./dist/store/index.js",
      "import": "./dist/store/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "nanobundle build",
    "clean": "nanobundle clean"
  },
  "devDependencies": {
    "nanobundle": "^1.5.0",
    "rimraf": "^4.4.0",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@4.1.1",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "ultra": {
    "concurrent": [
      "build"
    ]
  }
}
